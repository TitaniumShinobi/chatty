{
  "name": "api",
  "type": "module",
  "scripts": {
    "dev": "npx tsx server.js",
    "cli": "npx tsx cli-server.ts",
    "start": "npx tsx cli-server.ts",
    "prestart": "node -e \"const fs=require('fs');const path=require('path');const distPath=path.join(__dirname,'dist/engine/optimizedZen.js');if(process.env.NODE_ENV==='production'&&!fs.existsSync(distPath)){console.error('ERROR: Build artifacts missing in production. Run: npm run build');process.exit(1);}\"",
    "build": "tsc --project tsconfig.json",
    "prebuild": "echo 'Building OptimizedZenProcessor and related TypeScript files...'",
    "build:watch": "tsc --project tsconfig.json --watch"
  },
